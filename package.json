{
  "name": "lnkbox",
  "version": "0.1.0-ALPHA",
  "description": "A web-app and desktop tool for keeping track of bookmarks, links and archived content.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "dev:web": "cross-env NODE_ENV=development nodemon --exec ts-node ./src/index.ts",
    "build:web": "cross-env NODE_ENV=production tsc && webpack --config ./build/webpack.prod.js",
    "heroku-postbuild": "npm run build:web",
    "start": "cross-env NODE_ENV=production node ./dist"
  },
  "keywords": [
    "bookmark",
    "link",
    "archive",
    "url"
  ],
  "author": "Jacob Beneski",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^13.13.9",
    "@types/supertest": "^2.0.10",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "terser-webpack-plugin": "^3.0.2",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.0.8"
  },
  "nodemonConfig": {
    "watch": "./src"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@riophae/vue-treeselect": "^0.4.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bulma": "^0.9.0",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "lokijs": "^1.5.8",
    "morgan": "^1.10.0",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.3.2",
    "vuex": "^3.4.0"
  }
}
